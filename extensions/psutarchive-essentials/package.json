{
  "name": "psutarchive-essentials",
  "description": "Please enter a description for your extension",
  "icon": "extension",
  "version": "1.0.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-extension-hook"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "directus:extension": {
    "type": "hook",
    "path": "dist/index.js",
    "source": "src/index.ts",
    "host": "^10.10.0"
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link"
  },
  "devDependencies": {
    "@directus/types": "10.1.6",
    "@types/node": "^22.10.10",
    "typescript": "^5.7.3",
    "@types/pino": "^7.0.5"
  },
  "dependencies": {
    "@directus/extensions-sdk": "^13.0.0",
    "yaml": "^2.7.0"
  }
}
